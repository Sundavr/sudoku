            <Font size="20.0" />
         </font>
      </CheckBox>
      <CheckBox fx:id="generatorCheckBox" layoutX="340.0" layoutY="160.0" mnemonicParsing="false" onAction="#generatorCheckBoxAction" prefHeight="30.0" prefWidth="19.0" selected="true">
